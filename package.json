{
  "name": "cybersecurity-compliance-platform",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "Comprehensive Cybersecurity compliance platform with assessment, evidence collection, and certification readiness monitoring",
  "keywords": [
    "cmmc",
    "dod",
    "cybersecurity",
    "certification",
    "compliance",
    "assessment",
    "cui"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "analyze": "npx vite-bundle-analyzer",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "security:audit": "npm audit --audit-level high",
    "security:check": "npx audit-ci --config .audit-ci.json",
    "lighthouse": "lhci autorun",
    "build:analyze": "npm run build && npm run analyze",
    "validate": "npm run type-check && npm run lint && npm run test:run && npm run build",
    "clean": "rm -rf dist coverage test-results node_modules/.cache",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@supabase/supabase-js": "^2.53.0",
    "@types/node": "^24.1.0",
    "@vercel/analytics": "^1.5.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "dompurify": "^3.2.6",
    "jose": "^6.0.12",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.0",
    "recharts": "^3.1.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@lhci/cli": "^0.12.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "audit-ci": "^7.0.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^1.0.4"
  }
}
